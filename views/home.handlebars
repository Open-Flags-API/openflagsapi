
<div class="container-fluid">
  <div class="row row-cols-7">
    {{# each flagInfo}}


    <div class="col mx-auto border" id="flagBox" style="margin: .5rem;">
    <a href="/{{this.country}}/region/{{this.quickLink}}"> <img src="/{{this.country}}/region/{{this.quickLink}}" class="img-fluid" alt="Responsive image"></a>
      <p>Country: {{this.country}}</p>
      <p>Region: {{this.region}}</p>

      <a class="badge badge-primary text-wrap" style="width: 6rem;" href="/{{this.country}}/region/{{this.quickLink}}">
        Direct Link</a>
      <a class="badge badge-primary text-wrap" style="width: 6rem;" href="/api/{{this.country}}/{{this.region}}">
        API JSON</a>

    </div>

    <div class="col">
    </div>

    {{/each}}
  </div>
</div>